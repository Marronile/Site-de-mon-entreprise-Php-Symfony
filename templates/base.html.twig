<head>
    <meta charset="utf-8" />
    <link rel="icon" type="image/png" href="./images/favicon.ico">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <title>{% block title %}{% endblock %}  - Marronile</title>
    <meta content='width=device-width, initial-scale=1.0, shrink-to-fit=no' name='viewport' />

    {# stylesheets sections #}
     {% block stylesheets %}
    <link rel="stylesheet" type="text/css"
        href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700|Roboto+Slab:400,700|Material+Icons" />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css">
    <link href="./css/material-kit.css?v=2.0.7" rel="stylesheet" />
    {% endblock %}
</head>


{# Body begin #}
<body class="index-page sidebar-collapse">
    
    {# Navigation link #}
    <nav class="navbar navbar-transparent navbar-color-on-scroll fixed-top navbar-expand-lg" color-on-scroll="100"
        id="sectionsNav">
        <div class="container">
            <div class="navbar-translate">
                <a class="navbar-brand" href="/">
                    Marronile</a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" aria-expanded="false"
                    aria-label="Toggle navigation">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="navbar-toggler-icon"></span>
                    <span class="navbar-toggler-icon"></span>
                    <span class="navbar-toggler-icon"></span>
                </button>
            </div>
            <div class="collapse navbar-collapse">
                <ul class="navbar-nav ml-auto">
                    <li class="dropdown nav-item">
                        <a href="/" class=" nav-link">
                            <i class="material-icons">home</i> Accueil
                        </a>
                    </li>
                    <li class="dropdown nav-item">
                        <a href="/services" class=" nav-link">
                            <i class="material-icons">extension</i> Nos services
                        </a>
                    </li>
                    <li class="dropdown nav-item">
                        <a href="/about" class=" nav-link">
                            <i class="material-icons">info</i> A propos de nous
                        </a>
                    </li>
                    <li class="dropdown nav-item">
                        <a href="/contact" class=" nav-link">
                            <i class="material-icons">mail</i> Contact
                        </a>
                    </li>
                </ul>
            </div>
        </div>
    </nav>

    {# Page header #}
    {% block page_header %}{% endblock %}

    {# Page content #}
    <div class="main main-raised">
        {% block page_content %}{% endblock %}
    </div>

    {# Footer #}
    <footer class="footer" data-background-color="black">
        <div class="container">
            <nav class="float-left">
                <ul>
                    <li>
                        <a href="/">
                            Accueil
                        </a>
                    </li>
                    <li>
                        <a href="/about">
                            A propos
                        </a>
                    </li>
                    <li>
                        <a href="/services">
                            Nos services
                        </a>
                    </li>
                    <li>
                        <a href="/contact">
                            Contact
                        </a>
                    </li>
                </ul>
            </nav>
            <div class="copyright float-right">
                &copy;
                <script>
                    document.write(new Date().getFullYear())
                </script> Marronile par
                <a href="https://www.marronile.com/" target="_blank">Justin Dah-kenangnon</a>.
            </div>
        </div>
    </footer>

    {# javascripts sections #}
    {% block javascripts %}
    <script src="./javascripts/core/jquery.min.js" type="text/javascript"></script>
  <script src="./javascripts/core/popper.min.js" type="text/javascript"></script>
  <script src="./javascripts/core/bootstrap-material-design.min.js" type="text/javascript"></script>
    <script src="./javascripts/material-kit.js?v=2.0.7" type="text/javascript"></script>
    {% endblock %}
</body>
{# Body end
 #}</html>
